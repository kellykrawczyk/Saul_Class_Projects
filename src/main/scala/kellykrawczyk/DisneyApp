package kellykrawczyk

import Reader.DisneyReader
import edu.illinois.cs.cogcomp.lbjava.learn.SupportVectorMachine
import edu.illinois.cs.cogcomp.ner.ClassifiersAndUtils.DocumentReader
import edu.illinois.cs.cogcomp.saul.classifier.Learnable
import weka.classifiers.bayes.NaiveBayes
import Reader.reader.DisneyStock
import edu.illinois.cs.cogcomp.saul.classifier.ClassifierUtils
import edu.illinois.cs.cogcomp.saul.util.Logging
import kellykrawczyk.disney.DisneyDataModel._

import scala.collection.JavaConversions._


object DisneyApp extends App {

  val trainData = new DocumentReader("data/stocks/train").docs.toList
  val testData = new DocumentReader("data/stocks/test").docs.toList

  DisneyDataModel.disney.populate(trainData)
  FirstClassifier.learn(30)
  FirstClassifier.test(testData)



}